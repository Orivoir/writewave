{
  "name": "writewave",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "docker-compose up -d && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "seed-dev": "NODE_ENV=development npx tsx ./src/scripts/ensureDefaultEbookTheme.ts && NODE_ENV=development npx tsx ./src/scripts/ensureS3buckets.ts",
    "seed-prod": "NODE_ENV=prod npx tsx ./src/scripts/ensureDefaultEbookTheme.ts && NODE_ENV=prod npx tsx ./src/scripts/ensureS3buckets.ts",
    "fixtures:ebooks": "NODE_ENV=development npx tsx ./src/scripts/fixtures/ebooks.ts",
    "fixtures:ebooks-theme": "NODE_ENV=development npx tsx ./src/scripts/fixtures/ebooks-theme.ts",
    "fixtures:characters": "NODE_ENV=development npx tsx ./src/scripts/fixtures/characters.ts",
    "fixtures:notes": "NODE_ENV=development npx tsx ./src/scripts/fixtures/notes.ts",
    "fixtures:places": "NODE_ENV=development npx tsx ./src/scripts/fixtures/places.ts",
    "fixtures:assets": "NODE_ENV=development npx tsx ./src/scripts/fixtures/user-assets.ts",
    "fixtures:chapters": "NODE_ENV=development npx tsx ./src/scripts/fixtures/chapters.ts",
    "fixtures:all": "npm run fixtures:ebooks && npm run fixtures:ebooks-theme && npm run fixtures:characters && npm run fixtures:notes && npm run fixtures:places && npm run fixtures:assets && npm run fixtures:chapters"
  },
  "dependencies": {
    "@auth/mongodb-adapter": "^3.9.1",
    "@aws-sdk/client-s3": "^3.832.0",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/dm-serif-display": "^5.2.6",
    "@fontsource/work-sans": "^5.2.6",
    "@mui/icons-material": "^7.1.2",
    "@mui/material": "^7.1.2",
    "@mui/material-nextjs": "^7.1.1",
    "@tanstack/react-query": "^5.81.5",
    "dotenv": "^16.5.0",
    "file-type": "^21.0.0",
    "formidable": "^3.5.4",
    "mime-types": "^3.0.1",
    "mongoose": "^8.16.0",
    "next": "15.3.3",
    "next-auth": "^4.24.11",
    "next-intl": "^4.3.1",
    "nodemailer": "^6.10.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^3.0.2",
    "slugify": "^1.6.6",
    "stripe": "^18.3.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@faker-js/faker": "^9.8.0",
    "@types/formidable": "^3.4.5",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "node-fetch": "^3.3.2",
    "typescript": "^5"
  }
}
